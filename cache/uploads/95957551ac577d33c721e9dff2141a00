    function sanitizeFileName(name) {
  return name
    .trim()                         
    .replace(/\s+/g, '-')
    .replace(/\//g, '');
}

document.querySelectorAll('#imgList li .nameinpx').forEach(container => {
  container.addEventListener('input', function(e) {
    if (e.target.classList.contains('nameinpx')) {
      let fname = 'Pi7_Image_Tool';
      if (e.target.value.trim() !== '') {
        fname = sanitizeFileName(e.target.value);
      }
      if(fname.trim()=='')fname = 'Pi7_Image_Tool'
      let xname = e.target.getAttribute('fname')
      const parent = e.target.closest('li');
      const filnn = parent.querySelector('.imgxname');

      const alink = parent.querySelector('.downbtn');

      let newnme = fname + '.' + xname.split('.').pop()
    
      filnn.textContent = newnme;

      let modifiedUrl = xname.replace(/(\/)(CusRM)/, `$1${newnme}+_+_x$2`)

      alink.setAttribute('href',modifiedUrl)

      
    }
  });
});


    async function downall(){
    
    document.getElementById('spinneroverl').classList.remove('hide')
    
    const zip = new JSZip();
    
    for(img of loadedimages){
        
        await fetch('https://imback.pi7.org/'+img.filename.replace('static','downloadimage'))
        .then(response => response.blob())
        .then(blob => {
            const fileName = `${img.filename.split('/').pop().slice(11)}`;
            zip.file(fileName, blob);
        })
        .catch(error => {
            console.error('Error fetching image:', error);
        });
        
    };

    zip.generateAsync({ type: 'blob' }).then((content) => {
    // Create a download link
    const downloadLink = document.createElement('a');
    downloadLink.href = URL.createObjectURL(content);
    downloadLink.download = 'Pi7_Image_Tool.zip';

    // Hide the spinner overlay
    document.getElementById('spinneroverl').classList.add('hide');

    // Simulate a click on the download link to trigger the download
    downloadLink.click();
});


  }

